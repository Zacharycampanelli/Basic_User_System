<article class="post">
    <div class="post-title">
        <a href="/post/{{post.id}}" target="_blank">{{post.title}}</a>
    </div>
    <div class="post-body">
        {{post.bodyText}}
        {{post.photoUrl}}
    </div>
    <div class="post-info">
        {{post.user.username}} on {post.created_at}
    </div>
</article>